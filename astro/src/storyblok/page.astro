---
import Switcher from "./switcher.astro";
import PageComp from "../react/page";

interface Props {
  blok: {
    component: string;
    [field: string]: any;
  };
}

type Blok = {
  component: string;
  [field: string]: any;
};
interface Props {
  blok: Blok;
}

const { blok } = Astro.props;
---

<PageComp>
  {blok.body.map((bl: Blok) => <Switcher blok={bl} />)}
</PageComp>
