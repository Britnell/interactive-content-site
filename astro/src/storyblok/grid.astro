---
import Switcher from "./switcher.astro";
import RGrid from "../react/grid";

type Blok = {
  component: string;
  [field: string]: any;
};
interface Props {
  blok: Blok;
}

const { blok } = Astro.props;
---

<RGrid client:load>
  {blok.columns.map((bl: Blok) => <Switcher blok={bl} />)}
</RGrid>
